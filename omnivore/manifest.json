{
  "name": "Omnivore",
  "short_name": "Omnivore",
  "description": "Save links to your Omnivore library",
  "start_url": "/",
  "scope": "/",
  "dir": "ltr",
  "capture_links": ["existing-client-navigate", "new-client", "none"],
  "categories": [],
  "url_handlers" : [{
    "origin": "omnivore.app"
  }, {
    "origin": "beta.omnivore.app"
  }, {
    "origin": "demo.omnivore.app"
  }],
  "share_target": {
    "action": "/share-target",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "display": "standalone",
  "display_override": ["standalone", "minimal-ui", "browser"],
  "background_color": "#3D3D3D",
  "screenshots": [{
    "src": "/static/media/about/library-mobile.png",
    "type": "image/png",
    "sizes": "1500x2668"
  }],
